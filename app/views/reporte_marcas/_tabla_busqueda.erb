<table class="decorated">
  <% if show_titulares %>
    <tr>
      <th class="_200" >Denominacion</th>
      <th class="_200" colspan="7"><%= @reporte_marca.busqueda.upcase %></th>
    </tr>
    <tr>
      <th class="_200" >Fecha</th>
      <th class="_200" colspan="7"><%= l(@reporte_marca.created_at) %></th>
    </tr>
    <tr>
      <th class="_200" colspan="8"></th>
    </tr>
    <tr>
      <th class="_200" colspan="2">RESULTADOS</th>
    </tr>
  <% end %>
  <tr style="background-color: #000; color: #fff">
    <!--<th class="_200" style="padding: 4px;">Busqueda</th>-->
    <th class="_200">Signo</th>
    <th class="_200">Tipo</th>
    <th class="_200">Cl</th>
    <th class="_200">Numero</th>
    <th class="_200">Fecha</th>
    <% if show_titulares %>
      <th class="_200">Titulares</th>
      <th class="_200">Agentes</th>
    <% end %>
    <th>Comentario</th>
  </tr>
  <% @reporte_marca.reporte_marca_detalles.each do |rd|  %>
    <tr>
      <!--<td>
  <%#= rd.busqueda %>
      </td>-->
      <% if rd.marca_propia %>
      <td><span class="<%= rd.marca_propia.propia! %>"><%= rd.marca_propia.nombre%></span></td>
      <td><%= rd.marca_propia.tipo_signo.sigla if rd.marca_propia.tipo_signo %></td>
      <td><%= rd.marca_propia.clase_id %></td>
      <td><%= rd.marca_propia.numero_marca %></td>
      <td><%= rd.marca_propia.fecha_marca %></td>
      <%end%>
      <% if show_titulares %>
        <td><%= rd.marca_propia.titulares.join(", ") %></td>
        <td><%= rd.marca_propia.agentes.join(", ") %></td>
      <% end %>
      <td><%= rd.comentario %></td>
    </tr>
  <% end %>
  <% if show_titulares %>
    <tr>
      <th class="_200" colspan="8"></th>
    </tr>
    <tr>
      <td class="_200" >An√°lisis</th>
      <th class="_200" colspan="7"><%= @reporte_marca.carta %></th>
    </tr>
  <% end %>
    <tr>
      <td><%= render :partial => "nota" if show_titulares%></td>
    </tr>
</table>

