<% semantic_form_for @agente do |f| %>
  <%= f.input :nombre , :label => 'Nombre' %>
  <%= f.input :direccion, :as => :text, :label => 'Direccion', :input_html => {:rows => 3} %>
  <%= f.input :telefono , :label => 'Telefono' %>
  <%= f.input :email , :label => 'Email' %>
  <%= f.submit 'Salvar' %>
<% end %>
