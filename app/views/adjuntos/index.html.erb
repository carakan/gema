<h1>Listing adjuntos</h1>

<table>
  <tr>
    <th>Nombre</th>
  </tr>

<% @adjuntos.each do |adjunto| %>
  <tr>
    <td><%=h adjunto.nombre %></td>
    <td><%= link_to 'Show', adjunto %></td>
    <td><%= link_to 'Edit', edit_adjunto_path(adjunto) %></td>
    <td><%= link_to 'Destroy', adjunto, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New adjunto', new_adjunto_path %>