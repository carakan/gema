<h1>Importar Nueva <%= Marca::TIPOS[ params[:tipo] ] %></h1>


<% semantic_form_for( @marca, :url => solicitudes_path, :html => { :multipart => true } ) do |f| %>
  <%= hidden_field_tag('tipo', params[:tipo]) %>

  <div class="half fl"> 
    <%= f.input :archivo, :as => :file, :label => "Archivo" %>
  </div>

  <div style="clear:both"></div>

  <%= f.submit "Importar" %>
<% end %>
