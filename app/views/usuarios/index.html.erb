<h2>Listado de usuarios</h2>

<table class="decorated">
  <tr>
    <th>Nombre</th>
    <th>Rol</th>
    <th>Login</th>
    <th>Password</th>
    <th></th>
  </tr>

<% @usuarios.each do |usuario| %>
  <tr>
    <td><%=h link_to usuario.nombre, usuario %></td>
    <td><%=h usuario.rol %></td>
    <td><%=h usuario.login %></td>
    <td><%=h usuario.password %></td>
  
    <td><%= links (usuario) %> </td>
  </tr>

<% end %>
</table>

